{
  "name": "vite-mern-template",
  "version": "1.0.2",
  "main": "backend/src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "watch": "tsc --watch",
    "backend": "node --watch build/index.js",
    "dev:be": "concurrently \"npm run watch\" \"npm run backend\"",
    "dev:fe": "npm start --prefix ./frontend",
    "check-types": "tsc",
    "prettier": "prettier --write .",
    "build:backend": "tsc --project backend/tsconfig.json",
    "build:frontend": "npm run build --prefix ./frontend",
    "build": "tsc && npm run build:frontend"
  },
  "repository": "https://github.com/apicgg/vite-mern-template.git",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.13",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "firebase": "^12.4.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.25",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  }
}
